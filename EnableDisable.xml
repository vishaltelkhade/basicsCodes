<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EnableDisableSuite">
<parameter name="browserName" value="chrome"/>
<parameter name="username" value="Admin"/>
<parameter name="password" value="admin123"/>
<parameter name="URL" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
<parameter name="sleepTime" value="3000"/>
	<test name="EnableDisableTest">
		<classes>
			<class name="com.test.EnableDisableTest">
				<methods>
					<include name="launchUrl" />
					<include name="VerifyLogIn" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="EnableDisableTest2" enabled="false"> <!-- this testcase we enabled in this xml default it is, enabled="true" -->
		<classes>
			<class name="com.test.EnableDisableTest">
				<methods>
					<include name="launchUrl" />
					<include name="VerifyLogIn" />
					<include name="NavigateToInfo" />
					<include name="VerfyInfo" />
				</methods>
			</class>
		</classes>
	</test>
</suite>
