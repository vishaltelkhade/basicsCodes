<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- this is optional parameter example we dont provide any parameter value at line 24 in this xml-->
<!-- and at 12 we give parameter value as user name="browserName" value="chrome" /> then it has highest
presidence than default. so line no 12 will use chrome  -->
<suite name="SuiteOfParameter">
	<parameter name="username" value="Admin" />
	<parameter name="password" value="admin123" />
	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
	<parameter name="sleeptime" value="3000" />
	<test name="ParameterTest1">
		<parameter name="browserName" value="Edge" />
		<classes>
			<class name="com.test.OptionalParameterAnnotaion2">
				<methods>
					<include name="launchApp" />
					<include name="EnterLoginInfo" />
					<include name="verifylogin" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="ParameterTest2">
		<!--<parameter name="browserName" value="" /> -->
		<classes>
			<class name="com.test.OptionalParameterAnnotaion2">
				<methods>
					<include name="launchApp" />
					<include name="EnterLoginInfo" />
					<include name="verifylogin" />
					<include name="NavigateToInfo" />
					<include name="VerifyMyInfo" />
				</methods>
			</class>
		</classes>
	</test>
</suite>